{
  "info": {
    "name": "Baba Ji Pages API",
    "description": "Complete API collection for Baba Ji pages with posts, videos, and stories",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Baba Pages Management",
      "item": [
        {
          "name": "Create Baba Ji Page",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Baba Ramdev\",\n  \"description\": \"Yoga guru and spiritual leader\",\n  \"location\": \"Haridwar, India\",\n  \"religion\": \"Hinduism\",\n  \"website\": \"https://baba-ramdev.com\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/baba-pages",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages"]
            }
          }
        },
        {
          "name": "Get All Baba Ji Pages",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba-pages?page=1&limit=10&search=ramdev",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "search",
                  "value": "ramdev"
                }
              ]
            }
          }
        },
        {
          "name": "Get Specific Baba Ji Page",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}"]
            }
          }
        },
        {
          "name": "Update Baba Ji Page",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Baba Ramdev\",\n  \"description\": \"Updated description\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}"]
            }
          }
        },
        {
          "name": "Delete Baba Ji Page",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Posts Management",
      "item": [
        {
          "name": "Create Post",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "content",
                  "value": "This is a spiritual post about yoga and meditation",
                  "type": "text"
                },
                {
                  "key": "media",
                  "type": "file",
                  "src": []
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}/posts",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}", "posts"]
            }
          }
        },
        {
          "name": "Get All Posts",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}/posts?page=1&limit=10",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}", "posts"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "Get Specific Post",
          "request": {
            "method": "GET",
