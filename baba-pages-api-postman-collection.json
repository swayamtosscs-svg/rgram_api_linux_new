              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}", "posts", "{{post_id}}"]
            }
          }
        },
        {
          "name": "Update Post",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"content\": \"Updated post content\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}/posts/{{post_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}", "posts", "{{post_id}}"]
            }
          }
        },
        {
          "name": "Delete Post",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/api/baba-pages/{{page_id}}/posts/{{post_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba-pages", "{{page_id}}", "posts", "{{post_id}}"]
            }
          }
        }
      ]
    },
